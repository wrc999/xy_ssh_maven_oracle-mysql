<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
 <hibernate-mapping package="pojo">
	<class name="ReplyTalk" table="replytalk" >
		<id name="replytalk_id"  >
			<generator class="native"></generator>
		</id>
		<property name="use_id" column="use_id" ></property>
		<property name="talk_id" column="talk_id" ></property>
		<property name="replycontent" column="replycontent" ></property>
		<property name="replytime" column="replytime" ></property>
	</class>
 </hibernate-mapping>