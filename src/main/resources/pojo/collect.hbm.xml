<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
 <hibernate-mapping package="pojo">
	<class name="collect" table="collect" >
		<id name="collect_id"  >
			<generator class="native"></generator>
		</id>
		<property name="use_id" column="use_id" ></property>
		<property name="talk_id" column="talk_id" ></property>
		<property name="collectcontent" column="collectcontent" ></property>
		<property name="collecttime" column="collecttime" ></property>
		<property name="photo" column="photo" ></property>
	</class>
 </hibernate-mapping>